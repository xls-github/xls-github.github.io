    <div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>public void SelectionSort(int[] arr) {
     int len = arr.length;
     if (len &lt;= 0) {
         return;
     }
     for(int i=0;i&lt;len-1;i++){
         int minVal = i;
         for(int j=i+1;j&lt;=len-1;j++){
             if(arr[j]&lt;arr[minVal]){
                 minVal=j;
             }
         }
         if(minVal!=i){
             int temp = arr[i];
             arr[i]=arr[minVal];
             arr[minVal]=temp;
         }
			
     }
 }
</code></pre></div>    </div>
